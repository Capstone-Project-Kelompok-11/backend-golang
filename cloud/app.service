[Unit]
Description=Academy API
After=network.target nss-lookup.target postgresql.service

[Service]
ExecStart=/usr/bin/env DB_HOST=localhost DB_PORT=5432 DB_USERNAME=user DB_PASSWORD= DB_PASSWORD_FILE=/app/academy/config/db/postgres/passwd.txt DB_NAME=academy DB_CHARSET=utf8 DB_TIMEZONE=UTC DB_SECURE=false DB_UNIX_SOCK= /app/academy/bin/start
Restart=on-failure

[Install]
WantedBy=multi-user.target
